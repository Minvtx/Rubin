{
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60
            }
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 12 * * *"
        }
    ]
}